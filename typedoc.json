{
  "$schema": "https://typedoc.org/schema.json",
  "name": "deezer-ts",
  "entryPoints": ["src/index.ts"],
  "out": "./docs-site",
  "sort": ["source-order"],
  "searchCategoryBoosts": {
    "Component": 2,
    "Model": 1.2,
    "Exceptions": 1.5
  },
  "searchGroupBoosts": {
    "Classes": 1.5,
    "Error Handling": 2
  },
  "categoryOrder": [
    "Documentation",
    "API Reference",
    "Client",
    "PaginatedList",
    "Resources",
    "Errors"
  ],
  "navigation": {
    "includeCategories": true,
  },
  "hostedBaseUrl": "https://deezer-ts.guics.st/",
  "navigationLinks": {
    "Docs": "https://deezer-ts.guics.st/",
    "API": "https://deezer-ts.guics.st/api/index.html",
    "GitHub": "https://github.com/GuiEpi/deezer-ts"
  },
  "projectDocuments": ["docs/*.md"],
  "highlightLanguages": [
    "typescript",
    "tsx",
    "css",
    "json",
    "jsonc",
    "python",
    "yaml",
    "markdown"
  ],
  "markdownItOptions": {
    "html": true
  },
  "suppressCommentWarningsInDeclarationFiles": true
}